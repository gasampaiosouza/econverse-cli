{
  "name": "econverse",
  "description": "An amazing CLI made exclusively for Econverse.",
  "version": "1.2.0",
  "author": "Gabriel Sampaio @gasampaiosouza",
  "bin": {
    "econverse": "./bin/run"
  },
  "bugs": "https://github.com/gasampaiosouza/econverse-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "fs-extra": "^9.0.1",
    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/fs-extra": "^9.0.1",
    "@types/node": "^12.12.7",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
